module foo_proc(
  input wire [31:0] dir,
  input wire [31:0] in,
  input wire in_vld,
  input wire out1_rdy,
  input wire out2_rdy,
  output wire [31:0] out1,
  output wire [31:0] out2,
  output wire out1_vld,
  output wire out2_vld,
  output wire in_rdy
);
  wire out1_send_pred;
  wire out2_send_pred;
  wire literal_90;
  wire p0_all_active_outputs_ready;
  assign out1_send_pred = dir == 32'h0000_0000;
  assign out2_send_pred = ~out1_send_pred;
  assign literal_90 = 1'h1;
  assign p0_all_active_outputs_ready = (~out1_send_pred | out1_rdy) & (~out2_send_pred | out2_rdy);
  assign out1 = in;
  assign out2 = in;
  assign out1_vld = in_vld & literal_90 & literal_90 & out1_send_pred;
  assign out2_vld = in_vld & literal_90 & literal_90 & out2_send_pred;
  assign in_rdy = p0_all_active_outputs_ready;
endmodule
